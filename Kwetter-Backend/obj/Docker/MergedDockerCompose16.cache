services:
  authenticationapi:
    build:
      context: C:\School\Leerjaar 3\Semester 6\Individueelproject\Kwetter-Backend\Kwetter-Backend
      dockerfile: AuthenticationAPI/AuthenticationAPI/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: AuthenticationAPI
    container_name: AuthenticationAPI
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: authenticationapi
    labels:
      com.microsoft.visual-studio.project-name: AuthenticationAPI
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/AuthenticationAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5020
      target: 80
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\sande\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\sande\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\sande\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
